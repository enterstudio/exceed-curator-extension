   "icons": {
      "128": "static/exceedlms.png"
   },
   "manifest_version": 2,
   "name": "Exceed Curator",
   "permissions": [ "contextMenus", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.3"
}
