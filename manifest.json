{
   "background": {
      "persistent": false,
      "scripts": [ "script.js" ]
   },
   "browser_action": {
      "default_icon": "static/exceedlms.png",
      "default_title": "Exceed Curator"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
